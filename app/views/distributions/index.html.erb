<h1>Listing distributions</h1>

<table>
  <thead>
    <tr>
      <th>No publications</th>
      <th>Preferred1</th>
      <th>Distribution date</th>
      <th>Misc information</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @distributions.each do |distribution| %>
      <tr>
        <td><%= distribution.no_publications %></td>
        <td><%= distribution.preferred1 %></td>
        <td><%= distribution.distribution_date %></td>
        <td><%= distribution.misc_information %></td>
        <td><%= link_to 'Show', distribution %></td>
        <td><%= link_to 'Edit', edit_distribution_path(distribution) %></td>
        <td><%= link_to 'Destroy', distribution, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Distribution', new_distribution_path %>
